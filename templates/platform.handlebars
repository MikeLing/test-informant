<div id="{{suite}}-{{platform}}-panel" class="panel panel-default">
    <div id="{{suite}}-{{platform}}-heading" class="panel-heading" role="tab" data-toggle="collapse" data-parent="#{{suite}}-accordion" data-target="#{{suite}}-{{platform}}-collapse">
        <h4 class="panel-title">
            <a>{{platform}}</a>
        </h4>
    </div>
    <div id="{{suite}}-{{platform}}-collapse" class="panel-collapse collapse" role="tabpanel" aria-labelled-by="{{suite}}-{{platform}}-heading">
        <div class="panel-body">
            <div class="panel panel-default">
                <ul>
                    <li>Total tests: {{total}}</li>
                    <li>Total active tests: {{totalActive}}</li>
                </ul>
            </div>
            <div id="{{suite}}-{{platform}}-accordion" class="panel-group" role="tablist" aria-multiselectable="true">
                <div id="{{suite}}-{{platform}}-skipped-panel" class="panel panel-default">
                    <div id="{{suite}}-{{platform}}-skipped-heading" class="panel-heading" role="tab" data-toggle="collapse" data-parent="#{{suite}}-{{platform}}-accordion" data-target="#{{suite}}-{{platform}}-skipped-collapse">
                        <h4 class="panel-title">
                            <a>skipped tests: {{totalSkipped}}</a>
                        </h4>
                    </div>
                    <div id="{{suite}}-{{platform}}-skipped-collapse" class="panel-collapse collapse" role="tabpanel" aria-labelled-by="{{suite}}-{{platform}}-skipped-heading">
                        <div class="panel-body">
                            <div class="list-group">
                                {{#each skipped}}
                                    <a href="{{testurl this ../suite}}" class="list-group-item">{{this}}</a>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
                <div id="{{suite}}-{{platform}}-added-panel" class="panel panel-default">
                    <div id="{{suite}}-{{platform}}-added-heading" class="panel-heading" role="tab" data-toggle="collapse" data-parent="#{{suite}}-{{platform}}-accordion" data-target="#{{suite}}-{{platform}}-added-collapse">
                        <h4 class="panel-title">
                            <a>added tests: {{totalAdded}}</a>
                        </h4>
                    </div>
                    <div id="{{suite}}-{{platform}}-added-collapse" class="panel-collapse collapse" role="tabpanel" aria-labelled-by="{{suite}}-{{platform}}-added-heading">
                        <div class="panel-body">
                            <div class="list-group">
                                {{#each added}}
                                    <a href="{{testurl this ../suite}}" class="list-group-item">{{this}}</a>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
                <div id="{{suite}}-{{platform}}-removed-panel" class="panel panel-default">
                    <div id="{{suite}}-{{platform}}-removed-heading" class="panel-heading" role="tab" data-toggle="collapse" data-parent="#{{suite}}-{{platform}}-accordion" data-target="#{{suite}}-{{platform}}-removed-collapse">
                        <h4 class="panel-title">
                            <a>removed tests: {{totalRemoved}}</a>
                        </h4>
                    </div>
                    <div id="{{suite}}-{{platform}}-removed-collapse" class="panel-collapse collapse" role="tabpanel" aria-labelled-by="{{suite}}-{{platform}}-removed-heading">
                        <div class="panel-body">
                            <div class="list-group">
                                {{#each removed}}
                                    <a href="{{testurl this ../suite}}" class="list-group-item">{{this}}</a>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>
        </div>
    </div> 
</div>
